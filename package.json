{
  "publisher": "SbokyZahodi",
  "name": "fsd-files",
  "displayName": "FSD Slices Generator",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.5.0",
  "description": "Fast way to create slices",
  "author": "Denis Dmitriev <https://github.com/SbokyZahodi>",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.84.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.GenerateSlice",
        "title": "FSD - Generate Slice"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.GenerateSlice",
          "when": "explorerResourceIsFolder",
          "group": "2_workspace"
        }
      ]
    },

    "configuration": {
      "properties": {
        "fsd-files.template": {
          "type": "string",
          "default": "vue",
          "enum": ["vue", "react", "svelte", "custom"],
          "description": "Select template"
        }

      }
    }

  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
